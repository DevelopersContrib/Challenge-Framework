	.jcenter {
    	float:left;
		width:100%;
		background-image: url(images/bkgrnd.png);
		background-repeat: repeat;
	}


	.jcenter_cont {
		 margin:-45px auto 0px auto;
   		position:relative;
		background:#FFF;
		width:960px;min-height:500px;
		
		border:1px solid #CCC;
		border-bottom:0px;
		
		-moz-box-shadow: 0 0 3px 0px #888;
		-webkit-box-shadow: 0 0 3px 0px#ccc;
		box-shadow: 0 0 3px 0px #ccc;

	}
	
	.jcenter_hdr {
		width:960px;
    	margin:0px auto;
	}
  
  #jcenter-left{width:600px;float:left;margin:0px;padding:0px;
  }
  .jcenter-inner{padding:10px;min-height:600px;}
  
  #jcenter-right{width:330px;float:left;margin:0px;padding:0px;background:url(http://d2qcctj8epnr7y.cloudfront.net/images/marvinpogi/bg-earthchallenge2.png) repeat-y top left;}
	
	.jsection {
		display:table;
		text-align:center;
		widht:100%;
	}
	
	.jsection-tab {
	   float:left;
	   width:230px;
	   background-color:#FFF;
	   margin:2px;
	   text-align:left;
	   
	}
	
	.jsection-tab p:first-child
	{
		font-weight:bold;
		text-align:left;
		
	}
	
	
	.jsection-tab p:last-child {
	font-family:Arial, Helvetica, sans-serif;
	font-weight:normal;
	font-size:12px;
	text-align:left;
	
	}
	
	
	
	
	
