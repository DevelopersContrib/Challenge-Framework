@font-face {
    font-family: 'BebasNeueRegular';
    src: url('/font/bebasneue-webfont.eot');
    src: url('/font/bebasneue-webfont.eot?#iefix') format('embedded-opentype'),
         url('font/bebasneue-webfont.woff') format('woff'),
         url('/font/bebasneue-webfont.ttf') format('truetype'),
         url('/font/bebasneue-webfont.svg#BebasNeueRegular') format('svg');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'uni_acc';
    src: url('/font/uni_acc-webfont.eot');
    src: url('/font/uni_acc-webfont.eot?#iefix') format('embedded-opentype'),
         url('/font/uni_acc-webfont.woff') format('woff'),
         url('/font/uni_acc-webfont.ttf') format('truetype'),
         url('/font/uni_acc-webfont.svg#uni_acc') format('svg');
    font-weight: normal;
    font-style: normal;
}



	



	body {
	font-family:Arial, Helvetica, sans-serif;
	margin:0px 0px 0px 0px;
	background-image:url(images/bkgrnd.png);
	background-repeat:repeat;
	}

	
.jwidth	h1 {
		font-family:Arial, Helvetica, sans-serif;
		font-weight:normal;
		display: block;
		font-size: 30px;
		line-height:36px;
		-webkit-margin-before: 0.67em;
		-webkit-margin-after: 0.67em;
		-webkit-margin-start: 0px;
		-webkit-margin-end: 0px;
		font-weight: bold;
		color: white;
		text-align: left;
}  

	.a-big-mnu
	{
	  color:#000000;
	  text-decoration:none;
	  background-color:transparent;
	}
	
	.a-big-mnu:hover
	{
		color:#780403;
		text-decoration:none;
		background-color:transparent;
	}
	
	
	
	li a:visited {
